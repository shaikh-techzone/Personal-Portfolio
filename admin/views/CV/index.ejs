<!DOCTYPE html>
<html lang="en">
  <!-- Head Start -->
  <%- include("../partials/CV/head") %>
  <!-- Head End -->
  <body>
    <!-- Spinner Start -->
    <div
      id="spinner"
      class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center"
    >
      <div
        class="spinner-border text-primary"
        style="width: 3rem; height: 3rem"
        role="status"
      >
        <span class="sr-only">Loading...</span>
      </div>
    </div>
    <!-- Spinner End -->

    <div class="container">
      <div class="row g-5">
        <!-- Profile Start -->
        <%- include("../partials/CV/profile") %>
        <!-- Profile End -->
        <div class="col-lg-8">
          <!-- About Start -->
          <%- include("../partials/CV/about") %>
          <!-- About End -->

          <!-- Education Start -->
          <%- include("../partials/CV/education") %>
          <!-- Education End -->

          <!-- Skills Start -->
          <%- include("../partials/CV/skills") %>
          <!-- Skills End -->

          <!-- Expericence Start -->
          <%- include("../partials/CV/experience") %>
          <!-- Expericence End -->

          <!-- Subscribe Start -->
          <%- include("../partials/CV/subscribe") %>
          <!-- Subscribe End -->

          <!-- Services Start -->
          <%- include("../partials/CV/services") %>
          <!-- Services End -->

          <!-- Portfolio Start -->
          <%- include("../partials/CV/portfolio") %>
          <!-- Portfolio End -->

          <!-- Certificates Start -->
          <%- include("../partials/CV/certificates") %>
          <!-- Certificates End -->

          <!-- Testimonial Start -->
          <%- include("../partials/CV/testimonials") %>
          <!-- Testimonial End -->

          <!-- Contact Start -->
          <%- include("../partials/CV/contact") %>
          <!-- Contact End -->

          <!-- Footer Start -->
          <%- include("../partials/CV/footer") %>
          <!-- Footer End -->
        </div>
      </div>
    </div>

    <!-- Back to Top -->
    <a href="#" class="back-to-top"><i class="fa fa-angle-double-up"></i></a>

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js"></script>
    <script src="CV/lib/wow/wow.min.js"></script>
    <script src="CV/lib/typed/typed.min.js"></script>
    <script src="CV/lib/easing/easing.min.js"></script>
    <script src="CV/lib/waypoints/waypoints.min.js"></script>
    <script src="CV/lib/counterup/counterup.min.js"></script>
    <script src="CV/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="CV/lib/isotope/isotope.pkgd.min.js"></script>
    <script src="CV/lib/lightbox/js/lightbox.min.js"></script>

    <!-- Contact Javascript File -->
    <script src="mail/jqBootstrapValidation.min.js"></script>
    <script src="mail/contact.js"></script>

    <!-- Template Javascript -->
    <script src="CV/js/main.js"></script>
  </body>
</html>
